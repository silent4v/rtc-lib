{
  "name": "rtc-lib",
  "version": "1.0.0",
  "description": "software develop kit for real-time interactive",
  "module": "es2020",
  "type": "module",
  "main": "index.js",
  "author": "YanHau <yhchen.space@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lib:build": "npm-run-all prebuild:tsc build:rollup",
    "build:tsc": "tsc -p ./tsconfig.json",
    "build:rollup": "rollup -c",
    "lib:dev": "npm-run-all dev:watch dev:demo",
    "dev:watch": "tsc -p ./tsconfig.json -w",
    "dev:demo": "node serve.js",
    "docs:build": "jsdoc -c jsdoc.json",
    "docs": "npm-run-all build:tsc docs:build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@types/ws": "^8.2.0",
    "docdash": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "^7.0.2",
    "taffydb": "^2.7.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "fast-json-parse": "^1.0.3",
    "ws": "^8.2.3"
  }
}
